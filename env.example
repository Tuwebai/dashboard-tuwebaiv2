# Variables de entorno para Dashboard TuWebAI

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Gemini AI - Sistema Multi-API
# Configura hasta 5 API keys para fallback automático
VITE_GEMINI_API_KEY_1=your_primary_gemini_api_key_here
VITE_GEMINI_API_KEY_2=your_secondary_gemini_api_key_here
VITE_GEMINI_API_KEY_3=your_tertiary_gemini_api_key_here
VITE_GEMINI_API_KEY_4=your_fourth_gemini_api_key_here
VITE_GEMINI_API_KEY_5=your_fifth_gemini_api_key_here

# Configuración alternativa (compatibilidad con versión anterior)
REACT_APP_GEMINI_API_KEY=your_legacy_gemini_api_key_here

# Configuración de la aplicación
VITE_PUBLIC_URL=https://dashboard.tuweb-ai.com
VITE_APP_NAME=Dashboard TuWebAI
VITE_APP_VERSION=1.0.0

# Google Calendar Integration
VITE_GOOGLE_API_KEY=your_google_api_key_here
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here
VITE_GOOGLE_REDIRECT_URI=http://localhost:8083/auth/google/callback

# GitHub OAuth Integration
VITE_GITHUB_CLIENT_ID=your_github_client_id_here
VITE_GITHUB_CLIENT_SECRET=your_github_client_secret_here
# Para desarrollo: http://localhost:8083/auth/github/callback
# Para producción: https://dashboard.tuweb-ai.com/auth/github/callback
VITE_GITHUB_REDIRECT_URI=https://dashboard.tuweb-ai.com/auth/github/callback
VITE_GITHUB_TOKEN=your_github_personal_access_token_here

# LinkedIn OAuth Integration
VITE_LINKEDIN_CLIENT_ID=your_linkedin_client_id_here
VITE_LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret_here

# Encryption Key for Token Storage (opcional - se genera automáticamente si no se proporciona)
VITE_ENCRYPTION_KEY=your_32_character_encryption_key_here

# Configuración de desarrollo
NODE_ENV=development
VITE_DEBUG_MODE=true
